pipeline {
    agent any
    
    stages {
        stage('Build') {
            steps {
                sh 'echoing "Building..."'
            }
        }
        stage('Test') {
            steps {
                sh 'echoing "Testing..."'
            }
        }
        stage('Deploy') {
            steps {
                sh 'echoing "Deploying..."'
            }
        }
    }
}